
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DashBoard</title>
</head>
<body>
    <div> 
        welcome to ncr @Session["StaffId"]
      

        @using (Html.BeginForm("Billing", "StaffLogin", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Billing" class="btn btn-default" />
                    </div>
                </div>
            </div>
        }

        @using (Html.BeginForm("ShowStock", "StaffLogin", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Show Stock" class="btn btn-default" />
                    </div>
                </div>
            </div>
        }
        


    </div>
</body>
</html>
